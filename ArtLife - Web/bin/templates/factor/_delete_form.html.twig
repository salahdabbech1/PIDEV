<form method="post" action="{{ path('factor_delete', {'id': factor.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ factor.id) }}">
    <button class="btn btn-sm btn-danger"><i class="fa fa-trash"></i></button>
</form>
