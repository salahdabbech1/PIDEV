<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="crudaziz.MusicianController">
    <children>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="724.0" prefWidth="1067.0">
         <children>
            <VBox layoutX="134.0" layoutY="61.0" prefHeight="334.0" prefWidth="152.0" spacing="25.0">
               <children>
                  <Label fx:id="label1" minHeight="16" minWidth="69" />
                  <Label prefHeight="17.0" prefWidth="48.0" text="ID" />
                  <Label text="Nom" />
                  <Label text="Prenom" />
                  <Label text="Date de naissance" />
                  <Label text="Description" />
                  <Label text="Image" />
               </children>
            </VBox>
            <VBox layoutX="335.0" layoutY="102.0" prefHeight="221.0" prefWidth="166.0" spacing="16.0">
               <children>
                  <TextField fx:id="fxid" />
                  <TextField fx:id="fxname" />
                  <TextField fx:id="fxpr" />
                  <DatePicker fx:id="fxdate" />
                  <TextField fx:id="fxdesc" />
                  <TextField fx:id="fximage" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </VBox>
            <TableView fx:id="TableMusician" layoutX="555.0" layoutY="49.0" onMouseClicked="#SelectRow" prefHeight="357.0" prefWidth="508.0">
               <columns>
                  <TableColumn fx:id="ColID" prefWidth="67.0" text="ID" />
                  <TableColumn fx:id="ColName" prefWidth="144.0" text="Name" />
                  <TableColumn fx:id="Colpr" prefWidth="66.0" text="Prenom" />
                  <TableColumn fx:id="ColDate" prefWidth="104.0" text="Date de naissance" />
                  <TableColumn fx:id="ColDesc" prefWidth="95.0" text="Description" />
                  <TableColumn fx:id="ColIm" prefWidth="75.0" text="Image" />
               </columns>
            </TableView>
            <Button fx:id="BtnAdd" layoutX="185.0" layoutY="443.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Add" />
            <Button fx:id="BtnRefresh" layoutX="701.0" layoutY="455.0" mnemonicParsing="false" onAction="#showMusician" text="Refresh" />
            <Button fx:id="RemoveRow" layoutX="831.0" layoutY="455.0" mnemonicParsing="false" onAction="#removeRow" text="Remove Row" />
            <Label fx:id="Label" layoutX="300.0" layoutY="362.0" text="Label" />
            <Button fx:id="BtnModify" layoutX="376.0" layoutY="445.0" mnemonicParsing="false" onAction="#ModifyRow" text="Modifier" />
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
