   <children>
      <PieChart fx:id="chart" labelLineLength="30.0" layoutX="50.0" layoutY="42.0" legendSide="LEFT" prefHeight="254.0" prefWidth="500.0" title="Active Actors" />
      <Button fx:id="btnChart" layoutX="260.0" layoutY="337.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Load Pie Chart" />
      <Label fx:id="label" layoutX="90.0" layoutY="337.0" prefHeight="25.0" prefWidth="144.0" />
   </children>
</AnchorPane>
